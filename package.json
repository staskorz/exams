{
  "name": "exams",
  "version": "2.0.3",
  "private": true,
  "scripts": {
    "dev-server": "cross-env NODE_ENV=development nodemon --exec babel-node --watch server --watch common server/index.js",
    "dev-client": "webpack-dev-server --config webpack.dev.js",
    "build-server": "babel server -d dist/server && babel common -d dist/common",
    "build-client": "webpack --config webpack.prod.js -p",
    "prod": "node dist/server/index.js",
    "build-and-run-prod": "yarn run build-server && yarn run build-client && yarn run prod",
    "lint": "eslint ./client/ ./server/ ./common/ || true",
    "test": "jest",
    "test-watch": "jest --watch"
  },
  "dependencies": {
    "activedirectory": "^0.7.2",
    "dotenv": "^5.0.0",
    "express": "^4.16.0",
    "file-saver": "^1.3.2",
    "http-proxy-middleware": "^0.17.1",
    "lodash.debounce": "^4.0.8",
    "lodash.shuffle": "^4.2.0",
    "material-ui": "^0.20.0",
    "moment": "^2.15.1",
    "mongodb": "^3.0.2",
    "node-sspi": "^0.2.2",
    "papaparse": "^4.1.2",
    "pica": "^4.0.1",
    "prop-types": "^15.6.0",
    "randomstring": "^1.1.5",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-dropzone": "^4.2.7",
    "react-intl": "^2.4.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-virtualized": "9.18.0",
    "recompose": "^0.26.0",
    "redux": "^3.5.2",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.1.2",
    "babel-jest": "^22.1.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-react-jsx-source": "^6.22.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "copy-webpack-plugin": "^4.3.0",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.9",
    "eslint": "^4.16.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.6.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "jest": "^22.1.4",
    "nodemon": "^1.14.11",
    "react-hot-loader": "^3.1.3",
    "style-loader": "^0.20.1",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1",
    "webpack-merge": "^4.1.1"
  },
  "jest": {
    "roots": [
      "client/",
      "server/",
      "common/"
    ]
  }
}
